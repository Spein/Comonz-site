<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <script src="../js/commoners.js" type="module"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/firebase/firebase-app.js"></script>
    <script src="/js/firebase/firebase-database.js"></script>
    <link rel="stylesheet" href="/css/spectre.min.css">
    <link rel="stylesheet" href="/css/spectre-exp.min.css">
    <link rel="stylesheet" href="/css/spectre-icons.min.css">
    <link rel="stylesheet" href="/css/text-input.css">
    <link rel="stylesheet" href="/css/main.css">

    <link id="favicon" rel="shortcut icon" type="image/icon" href="/images/logos/logo-colour.png">


    <title>Commoner2</title>
</head>

<body>
    <div class="docs-demo columns" id="onProfile">
        <div class="column col-12 col-xs-12">
            <div class="panel">
                <div id="blackhole">
                </div>
                <i class="fas fa-file-import" style="display:none;margin: 18% 0px 0px 7%;position: absolute;" id="content-back"></i>
                <!--***/// Panels Nav ///*** -->
                <nav id="tabs" class="panel-nav">
                    <ul class="tab tab-block">
                        <li class="tab-item"><a class="tab-item-link active" id="profile" data-target="profile"><i
                                    style="margin:0 5%" class="far fa-user-circle"></i>Profile</a></li>
                        <li class="tab-item">
                            <a class="tab-item-link" id="wallet" data-target="wallet-i"><i style="margin:0 5%"
                                    class="fas fa-wallet"></i>Patronages</a></li>
                        <li class="tab-item"><a class="tab-item-link" id="transaction" data-target="wallet-o"><i
                                    style="margin:0 5%" class="fas fa-feather-alt"></i>Patrons</a></li>
                    </ul>
                </nav>
                <div id="panel-header" class="panel-header text-center">
                    <div id="notifications-h"></div>
                    <div class="header" id="profile-header">
                        <div class="logout-area">
                            <i class="fas fa-sign-out-alt" id="logout-button"></i>
                        </div>
                        <div class="square">
                            <div id="avatar-wrapper">
                                <div id="background"></div>
                                <div id="face"></div>
                                <div id="head"></div>
                                <div id="eye"></div>
                                <div id="mouth"></div>
                                <div id="clothes" style="border: 7px solid rgb(255 106 106)"></div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <div id="panels">
                <div class="panel-body profile active">
                    <div class="tile tile-centered vertical">

                        <div id="user-container" class="panel-title mt-10 editable">
                            <h3 id="displayName"></h3>
                        </div>
                        <div id="description-container" class="panel-titlemt-10 editable">
                            <h6 id="description"></h6>
                        </div>

                        <div id="nft-container" class="panel-title mt-10">
                            <p><b>NFT:</b><span id="nft-hash"></span></p>
                        </div>

                    </div>
                </div>
            </div>








            <script type="module" src="./js/onProfile.js" defer></script>



</body>